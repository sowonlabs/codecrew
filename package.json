{
  "name": "codecrew",
  "version": "0.2.1",
  "license": "MIT",
  "description": "CodeCrew: A Model Context Protocol (MCP) server for AI-powered code analysis and multi-agent collaboration",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "bin": {
    "codecrew": "dist/main.js"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "agents.yaml",
    "example.yaml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sowonlabs/codecrew"
  },
  "homepage": "https://github.com/sowonlabs/codecrew",
  "bugs": {
    "url": "https://github.com/sowonlabs/codecrew/issues"
  },
  "author": "SowonLabs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:mcp": "node dist/main.js mcp",
    "start:slack": "dotenv -e .env.slack -- node dist/main.js slack --log",
    "dev": "dotenv -e .env.test -- nest start --watch -- --log",
    "dev:install": "ts-node src/main.ts --install --log",
    "debug": "dotenv -e .env.test -- nest start --debug --watch -- --log --protocol HTTP",
    "inspector": "npx @modelcontextprotocol/inspector --config inspector.config.json --server codecrew",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "cli": "npm run build && node dist/main.js",
    "cli:help": "npm run build && node dist/main.js",
    "cli:init": "npm run build && node dist/main.js init",
    "cli:doctor": "npm run build && node dist/main.js doctor",
    "cli:query": "npm run build && node dist/main.js query",
    "cli:execute": "npm run build && node dist/main.js execute",
    "mcp": "npm run build && node dist/main.js mcp",
    "mcp:log": "npm run build && node dist/main.js mcp --log",
    "prepack": "npm run build && node -e \"const fs = require('fs'); const shebang = '#!/usr/bin/env node\\n'; const content = fs.readFileSync('dist/main.js', 'utf8'); fs.writeFileSync('dist/main.js', shebang + content);\""
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@slack/bolt": "^4.4.0",
    "@slack/web-api": "^7.10.0",
    "@sowonai/nestjs-mcp-adapter": "^0.1.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.4",
    "@types/unist": "^3.0.3",
    "chalk": "^4.1.2",
    "fast-glob": "^3.3.0",
    "handlebars": "^4.7.8",
    "ignore": "^5.2.0",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rxjs": "^7.8.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.19",
    "@types/yargs": "^17.0.0",
    "@vitest/ui": "^1.0.0",
    "dotenv-cli": "^6.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "codecrew",
    "mcp",
    "ai",
    "claude",
    "gemini",
    "code-analysis",
    "project-exploration",
    "model-context-protocol",
    "multi-agent",
    "ai-collaboration"
  ]
}
